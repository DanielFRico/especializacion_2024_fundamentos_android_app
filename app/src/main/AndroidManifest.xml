<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppFundamentos"
        tools:targetApi="31"
        android:name=".StoreAppApplication">
        <activity
            android:name=".core.ui.activity.SingUpActivity"
            android:exported="false"
            android:theme="@style/Theme.AppFundamentos"
            android:label="Registro"/>
        <activity
            android:name=".core.data.view.activity.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.AppFundamentos">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".core.ui.activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppFundamentos" >
        </activity>
    </application>

</manifest>